    "\n",
    "alphabet_data = []\n",
    "\n",
    "for i in range(1,27):\n",
    "    alphabet_data.append({'lower' : chr(i+96), 'upper' : chr(i+64), 'index' : i})\n",
    "    \n",
    "alphabet_df = pd.DataFrame(alphabet_data)\n",
    "alphabet_df.index += 1\n",
    "alphabet_df.drop(['index'],axis=1)\n",
    "alphabet_df"
